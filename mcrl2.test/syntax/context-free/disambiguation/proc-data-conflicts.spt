module processdataconflicts
language mcrl2
start symbol MCRL2Spec

test if-then and conj [[
  init a || b -> p;
]] parse to [[init a || (b -> p);]]

test if-then and add [[
  init a + b -> p;
]] parse to [[init a + (b -> p);]]

test if-then and element-at [[
  init a . b -> p;
]] parse to [[init a . (b -> p);]]

test if-then-else and conj [[
  init a || b -> p <> q;
]] parse to [[init a || (b -> p <> q);]]

test if-then-else and add [[
  init a + b -> p <> q;
]] parse to [[init a + (b -> p <> q);]]

test if-then-else and element-at [[
  init a . b -> p <> q;
]] parse to [[init a . (b -> p <> q);]]

test at and conj [[
  init p @ a || b;
]] parse to [[init (p @ a) || b;]]

test at and add [[
  init p @ a + b;
]] parse to [[init (p @ a) + b;]]

test at and element-at [[
  init p @ a . b;
]] parse to [[init (p @ a) . b;]]
